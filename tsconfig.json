{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
